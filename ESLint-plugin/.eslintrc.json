{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base","shani"], //eslint-config-shani
    // "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": ["promise"],
    "rules": {
        "promise/catch-or-return": "error",
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ]
        // "quotes": [
        //     "error",
        //     "single"
        // ],
        // "semi": [
        //     "error",
        //     "always"
        // ]
    }
}
